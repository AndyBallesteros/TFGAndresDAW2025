{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/pea-frontend/browser/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*\\.(?:js|css|map|json|ico|txt|svg|png|jpg|jpeg|gif|webmanifest|woff|woff2|ttf|eot))",
      "dest": "$1"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ],
  "outputDirectory": "dist/pea-frontend/browser"
}